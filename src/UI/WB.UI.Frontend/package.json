        "graphql-ws": "5.14.0",
        "glob-parent": "6.0.2",
        "jest-config": "29.0.3",
        "jest-environment-jsdom": "29.0.3",
        "json-schema": ">=0.4.0",
        "json5": "2.2.3",
        "yargs-parser": ">=13.1.2",
        "merge": ">=2.1.1",
        "moment": "2.29.4",
