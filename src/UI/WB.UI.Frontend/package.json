        "parse-url": "8.1.0",
        "rollup": "3.20.2",
        "sanitize-html": ">=2.12.1",
        "selfsigned": ">=1.3.0",
        "shelljs": ">=0.8.5",
        "svgo": ">=2.8.0",
        "trim-newlines": ">=3.0.1",
        "ts-node": "10.8.0",
        "vue-jest": ">=3.0.7",
        "ua-parser-js": "1.0.33",
        "ws": ">=8.18.0"
    }
